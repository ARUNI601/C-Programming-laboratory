#include<stdio.h>
int main()
{
    int r=0,t,n,rem;
    printf("enter number:");
    scanf("%d",&n);
    t=n;
    while (n!=0)
    {
        rem=n%10;
        r=(r*10)+rem;
        n=n/10;
    }
    if(r==t)
    {
        printf("it is a palindrome");
    }
    else
    {
        printf("it is not a palindrome");
    }
    
    return 0;
}
